#include <iostream>

using namespace std;

int main()
{

    int a,x,b,y,ile;

    cin>>ile;

    for (int i=0;i<ile;i++)
    {
        cin>>x>>y;

        a=x;
        b=y;

        while (a!=b)
            {
            if (a>b) a=a-b;
            else b=b-a;
            }
        cout<<(x*y)/a<<endl;
    }
    return 0;
}
