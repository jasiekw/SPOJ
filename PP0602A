#include <iostream>

using namespace std;

main()
{
int ile_testy, ile_liczb;

cin>>ile_testy;

for (int j=0;j<ile_testy;j++)
{
    cin>>ile_liczb;

    int tab[ile_liczb];

    for (int i=0;i<ile_liczb;i++) cin>>tab[i];


    for (int f=0;f<ile_liczb;f++)
    {
    if (f%2!=0) cout<<tab[f]<<" ";
    }

    for (int t=0;t<ile_liczb;t++)
    {
    if (t%2==0) cout<<tab[t]<<" ";
    }

}
return 0;
}

