#include <iostream>

using namespace std;

int main()
{
    int ile,tab[501],x;

    cin>>ile;
    cout<<endl;

    for (int i=0;i<ile;i++)

        {
            cin>>x;
            x*=x;
            tab[i]=x;
            cout<<endl;
        }

        cout<<endl;

    for (int j=0;j<ile;j++)

        {
            cout<<tab[j]<<endl;
            cout<<endl;
        }
    return 0;
}
